---
const token = '';
---

<div class="flex justify-end w-full gap-4 pr-4 md:w-max">
  {
    token ? (
      <p>Logged In</p>
    ) : (
      <a
        class="px-4 py-2 border-gray-300 rounded cursor-pointer border-1"
        id="signup"
        href="signup"
      >
        SignUp
      </a>
    )
  }
</div>
